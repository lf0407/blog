SQL: INSERT INTO `article` (`title`,`author`,`category`,`content`,`createtime`,`updatetime`) VALUES ('简易佐酒小菜---咖喱花蛤','西马栀子','美食','&lt;p style=&quot;text-align: center;&quot;&gt;&lt;img src=&quot;http://s5.sinaimg.cn/mw690/001JjjgZzy6TDd6Dmqo84&amp;amp;690&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;欢迎进入我的微博一起探讨美食&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;---&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;&lt;a href=&quot;http://http//weibo.com/u/1585670273&quot;&gt;西马栀子&lt;/a&gt;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;img src=&quot;http://s5.sinaimg.cn/mw690/001JjjgZzy6TDdnSxms54&amp;amp;690&quot; width=&quot;500&quot; height=&quot;669&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;这两天，天气很热，真心不爱做饭，做点简易的小菜，再买点现成的主食也就凑合了。买了一些花蛤，用水泡了一夜，今天就炒了它，再配上&lt;/span&gt;&lt;font&gt;2&lt;/font&gt;&lt;font&gt;瓶啤酒，也挺爽的。&lt;/font&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;font&gt;&lt;img src=&quot;http://s6.sinaimg.cn/mw690/001JjjgZzy6TDdtYXY105&amp;amp;690&quot; width=&quot;500&quot; height=&quot;674&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;喜欢吃海鲜，更喜欢&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;贝壳类的东西，因为&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;贝壳类海鲜&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;自身&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;就特别&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;鲜美，&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;尤其是花蛤，好吃还便宜，我&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;平时一般都是用来炖蛋，或者煮汤&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;再有就是&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;爆炒。&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;今天用浓香的咖喱汁&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;给它裹&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;起来，使花蛤的味道。更加鲜美浓香。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;咖喱&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;与&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;花蛤&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;的组合&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;，一种很奇妙的组合。当花蛤的鲜汁与咖喱的香气相溶的时候，吃在嘴里，会有一种满足感&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;。。。。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;img src=&quot;http://s15.sinaimg.cn/mw690/001JjjgZzy6TDdz9dUy6e&amp;amp;690&quot; width=&quot;500&quot; height=&quot;671&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;img src=&quot;http://s2.sinaimg.cn/mw690/001JjjgZzy6TDdAGi9r21&amp;amp;690&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;主料花蛤&amp;nbsp;&lt;font&gt;500&lt;/font&gt;&lt;font&gt;克，&lt;/font&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;辅料；姜碎&amp;nbsp;&lt;font&gt;3&lt;/font&gt;&lt;font&gt;克&amp;nbsp;&amp;nbsp;&lt;/font&gt;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;好侍百梦多咖喱&amp;nbsp;&lt;font&gt;2&lt;/font&gt;&lt;font&gt;块&lt;/font&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;做法；&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;img src=&quot;http://s11.sinaimg.cn/mw690/001JjjgZzy6TDdDisoWfa&amp;amp;690&quot; width=&quot;534&quot; height=&quot;399&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;1&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;准备好食材。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;2&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;花蛤用清水泡至&lt;font&gt;2&lt;/font&gt;&lt;font&gt;小时以上。&lt;/font&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;3&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;锅中注入油，下入姜碎炒香。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;4&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;加入一大勺清水。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;img src=&quot;http://s14.sinaimg.cn/mw690/001JjjgZzy6TDdF0jH7ed&amp;amp;690&quot; width=&quot;531&quot; height=&quot;392&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;5&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;加入&lt;font&gt;2&lt;/font&gt;&lt;font&gt;块咖喱。&lt;/font&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;6&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;烧至咖喱融化。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;7&amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;倒入洗净的花蛤。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;8&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;中火烧至，花蛤全部张开即可。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;img src=&quot;http://s12.sinaimg.cn/mw690/001JjjgZzy6TDdFXgXpfb&amp;amp;690&quot; alt=&quot;简易佐酒小菜---咖喱花蛤&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;叨唠；&lt;/span&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;花蛤要选用新鲜的，用淡盐水泡制&lt;font&gt;2&lt;/font&gt;&lt;font&gt;小时以上，然后清洗干净。爆炒的时候，不张开口的要捞出丢掉&lt;/font&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-size: 10.5pt;&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;','1528010999','1528010999') [ RunTime:0.0020s ]

