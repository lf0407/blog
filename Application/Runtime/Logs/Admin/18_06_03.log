SQL: INSERT INTO `article` (`title`,`author`,`category`,`content`,`createtime`,`updatetime`) VALUES ('不同脸型适合的太阳镜','月野丸子服装搭配','时尚','&lt;p&gt;&lt;font&gt;夏天阳光强烈，戴个墨镜又美观又可以遮挡强烈的阳光&lt;/font&gt;&lt;font&gt;挑选墨镜需要按照自己饿脸型来&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;不同脸型的人适合不同的墨镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;选好了适合自己的墨镜才会更好看更有范儿哦&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;&lt;br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;1.飞行员墨镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;飞行员墨镜适合长脸的MM&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s16.sinaimg.cn/mw600/6814252fge15cca80cdbf&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;RosieHuntington戴飞行员墨镜效果最好&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;NicoleRichie的脸型不太适合这款墨镜，她的脸型比较短、比较方&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s4.sinaimg.cn/mw600/6814252fge15cca96c863&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;阿汤哥的长脸也非常适合飞行员墨镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s14.sinaimg.cn/mw600/6814252fge15ccaa659dd&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;&lt;br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;2.方形太阳镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;方形太阳镜适合倒三角形脸和圆脸&lt;/p&gt;&lt;p&gt;&lt;font&gt;这两种脸型都是比较短的&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s1.sinaimg.cn/mw600/6814252fg7cefb172ebc0&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;RosieHuntington戴这款眼镜，脸过长了，不太合适&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;KateMoss的脸比较短，戴方形墨镜效果不错&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s5.sinaimg.cn/mw600/6814252fge15cee845094&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;&lt;br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;3.Wellington墨镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;粗框的墨镜，50年代很流行的款式&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;这款墨镜适合长脸的MM&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s16.sinaimg.cn/mw600/6814252fge15d083b5b2f&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;Rosie Huntington和Charlize Theron都是大长脸&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;戴这款墨镜效果比较好&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;&lt;br&gt;&lt;/font&gt;&lt;img src=&quot;http://s16.sinaimg.cn/mw600/6814252fge15d0842a61f&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;&lt;br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;4.椭圆形大镜片墨镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;这款墨镜适合鹅蛋脸或者方脸&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;鹅蛋脸和圆脸的不同之处在长度，圆脸比较短，鹅蛋脸长一些&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;瓜子脸再饱满一点就是鹅蛋脸，鹅蛋脸压扁就成了圆脸&lt;/font&gt;&lt;img src=&quot;http://www.sinaimg.cn/uc/myshow/blog/misc/gif/E___6725EN00SIGG.gif&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s13.sinaimg.cn/mw600/6814252fg7cefb6ac9a0c&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;Miranda Kerr和AnneHathaway比较适合椭圆大镜片墨镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;赫本、赵薇应该也比较适合这款墨镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;&lt;br&gt;&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;MM们可以根据自己的脸型可以挑选出适合自己的太阳镜&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;这样就不用一个一个去试了&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;喜欢的可以关注我的微博哦：&lt;a href=&quot;http://weibo.com/u/1746150703&quot; rel=&quot;nofollow&quot;&gt;http://weibo.com/u/1746150703&lt;/a&gt;&amp;nbsp;&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s10.sinaimg.cn/mw600/6814252fge15d22c9cef9&amp;amp;690&quot; alt=&quot;不同脸型适合的太阳镜&quot;&gt;&lt;/p&gt;','1528011384','1528011384') [ RunTime:0.0020s ]

