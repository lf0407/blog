SQL: INSERT INTO `article` (`title`,`author`,`category`,`content`,`createtime`,`updatetime`) VALUES ('华丽宴客菜 鲍鱼烩海参','孙贵喜sgx','美食','&lt;p&gt;&lt;img src=&quot;http://s15.sinaimg.cn/mw690/002jax0hzy7k6keEUge6e&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s11.sinaimg.cn/mw690/002jax0hzy7k6kfZezMea&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s5.sinaimg.cn/mw690/002jax0hzy7k6kkcMAc64&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;http://s11.sinaimg.cn/mw690/002jax0hzy7k6kiwKqC4a&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;/p&gt;&lt;p&gt;&lt;b&gt;&lt;span style=&quot;color: rgb(255, 0, 0); font-size: 16pt;&quot;&gt;&lt;font&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;华丽宴客菜&lt;/font&gt;&amp;nbsp;&lt;font&gt;鲍鱼烩海参&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;span style=&quot;font-size: 12pt;&quot;&gt;&lt;font&gt;朋友从远方来，不亦乐呼！做一道鲍鱼烩海参也是很有面子的事！鲍鱼海参都属海四珍，营养丰富，口感细腻，十分美味！因淡干海参和干鲍鱼都是预先发制，做这道菜也就省事了，成菜时间快，也不耽误与朋友说话。&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;span style=&quot;font-size: 12pt;&quot;&gt;&lt;font&gt;食材：特级已煮发好的海参&lt;/font&gt;1&lt;font&gt;根约&lt;/font&gt;&lt;font&gt;150&lt;/font&gt;&lt;font&gt;克，煮发好的干鲍鱼&lt;/font&gt;&lt;font&gt;4&lt;/font&gt;&lt;font&gt;只约&lt;/font&gt;&lt;font&gt;170&lt;/font&gt;&lt;font&gt;克，干贝&lt;/font&gt;&lt;font&gt;10&lt;/font&gt;&lt;font&gt;克，鲍鱼海参汁&lt;/font&gt;&lt;font&gt;2&lt;/font&gt;&lt;font&gt;袋，蚝油料酒酱油葱姜蒜盐糖鸡精 淀粉食用油适量&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;span style=&quot;font-size: 12pt;&quot;&gt;&lt;font&gt;做法：&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;span style=&quot;font-size: 12pt;&quot;&gt;1&lt;font&gt;、&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;font&gt;&lt;span style=&quot;font-size: 16px;&quot;&gt;&lt;b&gt;准备已煮发的海参，如要知如何发制淡干海参，&lt;a href=&quot;http://blog.sina.com.cn/s/blog_7e185e350102viam.html&quot; target=&quot;_blank&quot;&gt;可点击查看&lt;/a&gt;&lt;/b&gt;&lt;/span&gt;&lt;/font&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;span style=&quot;font-size: 12pt;&quot;&gt;&lt;font&gt;2、&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;font&gt;&lt;span style=&quot;font-size: 16px;&quot;&gt;&lt;b&gt;将海参洗净，因海参较大，剖成片备用，&lt;/b&gt;&lt;/span&gt;&lt;/font&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;span style=&quot;font-size: 12pt;&quot;&gt;&lt;font&gt;3、&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;font&gt;&lt;span style=&quot;font-size: 16px;&quot;&gt;&lt;b&gt;准备已煮发的干鲍鱼，如要知如何发制干鲍鱼，&lt;a href=&quot;http://blog.sina.com.cn/s/blog_7e185e350102x09d.html&quot; target=&quot;_blank&quot;&gt;可点击查看&lt;/a&gt;&lt;/b&gt;&lt;/span&gt;&lt;/font&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;img src=&quot;http://s10.sinaimg.cn/small/002jax0hzy7k6kvRW4h39&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&amp;nbsp;&lt;img src=&quot;http://s7.sinaimg.cn/small/002jax0hzy7k6kwXQmq66&amp;amp;690&quot; width=&quot;200&quot; height=&quot;123&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&amp;nbsp;&lt;img src=&quot;http://s4.sinaimg.cn/small/002jax0hzy7k6kxLyLNb3&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;4、准备调料，干贝要先洗净泡发30分钟，姜切片，葱切节，将其它调料兑成调汁&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;5、坐锅点中火注油，投入葱姜蒜炒黄煸香&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;6、倒入调汁和干贝烧开&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;img src=&quot;http://s16.sinaimg.cn/small/002jax0hzy7k6kH0N792f&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&amp;nbsp;&lt;img src=&quot;http://s3.sinaimg.cn/small/002jax0hzy7k6kIgGn882&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&amp;nbsp;&lt;img src=&quot;http://s2.sinaimg.cn/small/002jax0hzy7k6kJM7apf1&amp;amp;690&quot; width=&quot;200&quot; height=&quot;126&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;7、加入鲍鱼大火烧开，转小火煮20分钟&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;8、投入海参，翻炒一下烧开&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;9、烹入料酒进一步去腥，因海参是熟制品烧2分钟即可，收汁出锅&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;img src=&quot;http://s13.sinaimg.cn/small/002jax0hzy7k6kSuXA81c&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&amp;nbsp;&lt;img src=&quot;http://s5.sinaimg.cn/small/002jax0hzy7k6kTvpoUe4&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&amp;nbsp;&lt;img src=&quot;http://s1.sinaimg.cn/small/002jax0hzy7k6kUXnUsb0&amp;amp;690&quot; width=&quot;200&quot; height=&quot;130&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;br&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;b&gt;&lt;span style=&quot;color: rgb(255, 0, 0); font-size: 16pt;&quot;&gt;&lt;font&gt;华丽宴客菜&lt;/font&gt;&amp;nbsp;&lt;font&gt;鲍鱼烩海参&lt;/font&gt;&lt;/span&gt;&lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;img src=&quot;http://s2.sinaimg.cn/mw690/002jax0hzy7k6kmZEJj11&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;img src=&quot;http://s12.sinaimg.cn/mw690/002jax0hzy7k6koX74vdb&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;/p&gt;&lt;p style=&quot;margin-left: 0pt;&quot;&gt;&lt;img src=&quot;http://s6.sinaimg.cn/mw690/002jax0hzy7k6kqaKzj95&amp;amp;690&quot; alt=&quot;华丽宴客菜&amp;nbsp;&lt;wbr&gt;鲍鱼烩海参&quot;&gt;&lt;/p&gt;','1528009554','1528009554') [ RunTime:0.0030s ]

