SQL: INSERT INTO `article` (`title`,`author`,`category`,`content`,`createtime`,`updatetime`) VALUES ('红海日出红透海天','南麂土著','旅游','&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567bfd3e4b0.jpg&quot; height=&quot;449&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;在阿拉伯半岛和非洲大陆之间有一条形状狭长，色彩凝重的海域，它便是充满异域风情的红海了。西边是埃及东部沙漠，东边是黄沙滚滚的阿拉伯半岛沙漠，一半是火焰，一半是海水。&lt;br&gt;在埃及赫尔格达红海地区，延绵的沙漠干净利落地直接潜入碧蓝的海水，没有过渡，波澜不惊。&lt;br&gt;起一个大早等日出，看吧，原来红海是被浓烈的霞光映红的。&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c0364a8d.jpg&quot; height=&quot;451&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;font&gt;以下是赫尔格达海滩日出的过程，浓郁而瑰丽，地道的阿拉伯情怀：&lt;/font&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c23ba8aa.jpg&quot; height=&quot;448&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c2803b58.jpg&quot; height=&quot;450&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c2b73a5f.jpg&quot; height=&quot;457&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c2eaf3d5.jpg&quot; height=&quot;452&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c328f3d1.jpg&quot; height=&quot;457&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c3579a06.jpg&quot; height=&quot;456&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c3b6854f.jpg&quot; height=&quot;450&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/5c567c3bd545f.jpg&quot; height=&quot;455&quot; width=&quot;690&quot; alt=&quot;红海日出红透海天&quot;&gt;&lt;/p&gt;&lt;p&gt;&lt;img src=&quot;/Uploads/2019-02-03/